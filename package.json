{
  "name": "shelljs-transpiler",
  "version": "0.2.0",
  "description": "Translate bash to shelljs",
  "keywords": [
    "shelljs",
    "shell",
    "bash",
    "convert",
    "source",
    "translate",
    "transpile"
  ],
  "main": "./bin/transpile.js",
  "scripts": {
    "posttest": "npm run lint",
    "lint": "jshint .",
    "build": "ohm-builder.js src/index.generator.html index.html",
    "test": "node ./scripts/runtests.js",
    "after-travis": "travis-check-changes",
    "deploy": "deploy-gh"
  },
  "author": "Nate Fischer <ntfschr@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/nfischer/BashToShellJS"
  },
  "license": "MIT",
  "bin": {
    "shjs": "./bin/shjs"
  },
  "devDependencies": {
    "deploy-gh": "^0.1.1",
    "gulp": "^3.9.1",
    "jshint": "^2.9.2",
    "ohm-builder": "^0.3.0",
    "travis-check-changes": "^0.3.0"
  },
  "dependencies": {
    "minimist": "^1.2.0",
    "shelljs": "^0.7.0"
  }
}
